{
  "AWSTemplateFormatVersion": "2010-09-09",
  "Description": "BuildDictionary Artifacts Storage",
  "Parameters": {
    "BucketName": {
      "Type": "String"
    }
  },
  "Resources": {
    "Bucket": {
      "Type": "AWS::S3::Bucket",
      "Properties": {
        "BucketName": {"Ref": "BucketName"}
      }
    }
  },
  "Outputs": {
    "Bucket": {
      "Value": {
        "Ref": "Bucket"
      }
    }
  }
}

{
  "rules": {
    "no-string-throw": true,
    "no-unused-expression": true,
    "no-duplicate-variable": true,
    "curly": [true, "ignore-same-line"],
    "class-name": true,
    "semicolon": [
      false,
      "always"
    ],
    "quotemark": [true, "single", "avoid-escape"],
    "triple-equals": true
  },
  "defaultSeverity": "error"
}
